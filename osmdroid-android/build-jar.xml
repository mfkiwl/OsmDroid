<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse.ant.import?>
<project>
	<!--
		Can't make it depend on build because that's in a different build file.
		So just run
		ant build jar
	-->
	<target name="jar">
	    <jar destfile="osmdroid-android.jar"
	         basedir="bin"
	         includes="**/*.class"
	         />
	  </target>
</project>
